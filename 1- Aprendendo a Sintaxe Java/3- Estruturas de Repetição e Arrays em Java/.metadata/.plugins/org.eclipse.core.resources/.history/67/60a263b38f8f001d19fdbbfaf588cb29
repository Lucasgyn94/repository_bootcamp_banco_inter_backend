package desafios.arrays;

public class OrdemInversa {

	public static void main(String[] args) {
		int[] vetor = {-5, -6, 15, 50, 8, 4};
		
		System.out.println("Vetor: ");
		
		int contador = 0;
		
		while (contador < (vetor.length)) {
			System.out.println(vetor[contador] + " ");
			contador++;
		}

	}

}
